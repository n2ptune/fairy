{
  "name": "fairy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:firebase": "firebase serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "(cd app && yarn install); (cd functions && yarn install);",
    "patch:deps": "yarn upgrade -L && (cd app && yarn upgrade -L); (cd functions && yarn upgrade -L);",
    "deploy:hosting": "yarn build && firebase deploy --only hosting"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.7.0",
    "element-ui": "^2.14.1",
    "firebase": "^8.0.2",
    "prismjs": "^1.22.0",
    "uid": "^2.0.0",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.1",
    "vue-prism-component": "^1.2.0",
    "vue-router": "^3.4.9",
    "vue-simplemde": "^1.1.2",
    "vue-unicons": "^2.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-plugin-router": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^7.13.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.1.0",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^11.0.1",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "arrow-parens": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
